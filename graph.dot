graph Graph_coloring { 
	node [shape=circle width=0.66 style=filled]
	0 [color = magenta label="0"]
	1 [color = blue label="1"]
	2 [color = yellow label="2"]
	3 [color = magenta label="3"]
	4 [color = green label="4"]
	5 [color = red label="5"]
	0 -- 1;
	0 -- 2;
	0 -- 4;
	0 -- 5;
	1 -- 2;
	1 -- 4;
	1 -- 5;
	2 -- 3;
	2 -- 4;
	2 -- 5;
	3 -- 4;
	4 -- 5;
}